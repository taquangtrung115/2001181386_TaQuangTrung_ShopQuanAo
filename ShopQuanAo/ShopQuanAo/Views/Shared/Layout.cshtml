<body>
    <div class="wrap">
        <div class="header">
            <div class="headertop_desc">
                
                <div class="account_desc">
                    <ul>
                        <li><a href="@Url.Action("DangKy", "NguoiDung")">Đăng Ký</a></li>
                        <li><a href="@Url.Action("DangNhap", "NguoiDung")">Đăng Nhập</a></li>
                        
                    </ul>
                </div>
                <div class="clear"></div>
            </div>
            <div class="header_top">
                <div class="logo">
                    <a href="/" ref="nofollow"><img src="~/ContentsNguoiDung/images/logo.png" alt="" /></a>
                </div>
                @{Html.RenderAction("GioHang", "Home");}
                <script type="text/javascript">
                    function DropDown(el) {
                        this.dd = el;
                        this.initEvents();
                    }
                    DropDown.prototype = {
                        initEvents: function () {
                            var obj = this;

                            obj.dd.on('click', function (event) {
                                $(this).toggleClass('active');
                                event.stopPropagation();
                            });
                        }
                    }

                    $(function () {

                        var dd = new DropDown($('#dd'));

                        $(document).click(function () {
                            // all dropdowns
                            $('.wrapper-dropdown-2').removeClass('active');
                        });

                    });

                </script>
                <div class="clear"></div>
            </div>
            <div class="header_bottom">
                @RenderPage("Menu.cshtml")
                <div class="search_box">
                    <form>
                        <input type="text" value="Search" onfocus="this.value = '';" onblur="if (this.value == '') {this.value = 'Search';}"><input type="submit" value="">
                    </form>
                </div>
                <div class="clear"></div>
            </div>
            <div class="header_slide">
                <div class="header_bottom_left">
                    @{Html.RenderAction("DanhMuc", "DanhMuc");}
                </div>
                <div class="header_bottom_right">
                    <div class="slider">
                        <div id="slider">
                            <div id="mover">
                                <div id="slide-1" class="slide">
                                    <div class="slider">
                                        <a href="preview.html"><img src="~/ContentsNguoiDung/Image/banner.jpg" alt="learn more" /></a>
                                    </div>
                                    
                                    <div class="clear"></div>
                                </div>
                                <div class="slide">
                                    
                                    <div class="slider">
                                        <a href="preview.html"><img src="~/ContentsNguoiDung/Image/banner2.jpg" alt="learn more" /></a>
                                    </div>
                                    <div class="clear"></div>
                                </div>
                                <div class="slide">
                                    <div class="slider">
                                        <a href="preview.html"><img src="~/ContentsNguoiDung/Image/banner3.jpg" alt="learn more" /></a>
                                    </div>
                                    
                                    <div class="clear"></div>
                                </div>
                            </div>
                        </div>
                        <div class="clear"></div>
                    </div>
                </div>
                <div class="clear"></div>
            </div>
        </div>
        <div class="main">
            @RenderBody();
           
        </div>
    </div>
    <div class="footer">
        <div class="wrap">
            <div class="section group">
                <div class="col_4_of_8 span_4_of_8" style="text-align:center">
                    <h4>Liên Hệ</h4>
                    <ul>
                        <li><span>Email: trungquangta115@gmail.com</span></li>
                        <li><span>Phone: 0352393384</span></li>
                    </ul>
                    <div class="social-icons">
